# Minecraft Server Properties
# Generated by Docker Minecraft Server

# Server identification
motd=Welcome to my Minecraft Server!
server-name=Minecraft Server

# Network settings
server-ip=
server-port=25565

# Game settings
gamemode=survival
difficulty=easy
hardcore=false
pvp=true
allow-flight=false
allow-nether=true
force-gamemode=false
generate-structures=true
spawn-npcs=true
spawn-animals=true
spawn-monsters=true

# World settings
level-name=world
level-seed=
level-type=minecraft:normal
generator-settings=

# Player limits
max-players=20
online-mode=true
white-list=false
enforce-whitelist=false

# View distance
view-distance=10
simulation-distance=10

# Resource pack
resource-pack=
resource-pack-sha1=
require-resource-pack=false

# Server performance
max-tick-time=60000
network-compression-threshold=256

# Query and monitoring
enable-query=false
query.port=25565
enable-rcon=false
rcon.port=25575
rcon.password=

# Debugging
debug=false
broadcast-console-to-ops=true
broadcast-rcon-to-ops=true